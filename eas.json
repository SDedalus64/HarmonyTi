{
  "cli": {
    "version": ">= 13.2.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_NO_DEV_MENU": "false"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_NO_DEV_MENU": "true"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "env": {
        "EXPO_NO_DEV_MENU": "true"
      },
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
